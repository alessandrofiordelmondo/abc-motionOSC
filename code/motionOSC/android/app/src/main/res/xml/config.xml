<?xml version='1.0' encoding='utf-8'?>
<widget version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <access origin="*" />
  
  <feature name="Compass">
    <param name="android-package" value="org.apache.cordova.deviceorientation.CompassListener"/>
  </feature>

  <feature name="Insomnia">
    <param name="android-package" value="nl.xservices.plugins.Insomnia"/>
  </feature>

  <feature name="Magnetometer">
    <param name="android-package" value="org.apache.cordova.magnetometer.Magnetometer"/>
  </feature>

  <feature name="OSC">
    <param name="android-package" value="nl.sylvain.cordova.osc.OSCUtils"/>
  </feature>

  
</widget>